// Importa as bibliotecas necess√°rias do React
import React from 'react';

// Cria um componente funcional chamado App
function App() {
  // Define seu nome
  const meuNome = "Seu Nome";

  // Retorna o JSX desejado
  return (
    <div>
      <h1>Meu Nome: {meuNome}</h1>
    </div>
  );
}

// Exporta o componente App para ser utilizado em outros arquivos
export default App;
